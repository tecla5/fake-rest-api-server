{
  "type": "array",
  "uniqueItems": true,
  "items": {
    "properties": {
      "id": {
        "type": "string",
        "faker": "random.uuid"
      },
      "email": {
        "type": "string",
        "faker": "internet.email"
      },
      "firstname": {
        "type": "string",
        "faker": "name.firstName"
      },
      "lastname": {
        "type": "string",
        "faker": "name.lastName"
      },
      "username": {
        "type": "string",
        "faker": "internet.userName"
      },
      "emailVerified": {
        "type": "boolean"
      },
      "avatarImageUrl": {
        "type": "string",
        "faker": "image.avatar"
      },
      "splashImageUrl": {
        "type": "string",
        "faker": "image.avatar"
      },
      "deviceToken": {
        "type": "string"
      },
      "updatedAt": {
        "type": "string",
        "format": "date-time",
        "faker": "date.recent"
      },
      "createdAt": {
        "type": "string",
        "format": "date-time",
        "faker": "date.recent"
      },
      "address": {
        "properties": {
          "administrativeArea": {
            "type": "string",
            "faker": "address.state"
          },
          "formattedAddressLines": {
            "type": "string",
            "description": "Read-only, pretty printed string of the address",
            "faker": "address.streetAddress"
          },
          "locality": {
            "type": "string"
          },
          "thoroughfare": {
            "type": "string"
          },
          "postalCode": {
            "type": "string",
            "faker": "address.zipCode"
          },
          "name": {
            "type": "string",
            "faker": "commerce.productName"
          },
          "suite": {
            "type": "string"
          },
          "isoCountryCode": {
            "type": "string",
            "faker": "address.countryCode"
          }
        }
      },
      "paypalEmail": {
        "type": "string",
        "faker": "internet.email"
      },
      "phoneNumber": {
        "type": "string",
        "faker": "phone.phoneNumber"
      },
      "gender": {
        "type": "string",
        "enum": [
          "male",
          "female"
        ]
      },
      "birthday": {
        "type": "string",
        "format": "date"
      },
      "taxId": {
        "type": "string",
        "faker": "random.number"
      },
      "locale": {
        "type": "string",
        "faker": "random.locale"
      },
      "roles": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "enum": [
          "admin",
          "guest",
          "user"
        ]
      },
      "altAvatarImages": {
        "type": "object",
        "properties": {}
      },
      "profileBio": {
        "type": "string"
      },
      "allowsChat": {
        "type": "boolean"
      },
      "currency": {
        "type": "string",
        "faker": "finance.currencyCode"
      },
      "appUserType": {
        "type": "string",
        "enum": [
          "regular",
          "influencer",
          "retailer",
          "brand"
        ]
      },
      "isVerifiedByYeay": {
        "type": "boolean"
      },
      "yeayScore": {
        "type": "integer",
        "minimum": 0,
        "description": "Only available if user is own user or when in admin context",
        "faker": {
          "random.number": [
            1000
          ]
        }
      },
      "isScoreFrozen": {
        "type": "boolean",
        "description": "Only available if user is own user or when in admin context"
      }
    },
    "required": [
      "id",
      "email",
      "firstname",
      "lastname",
      "username",
      "emailVerified",
      "avatarImageUrl",
      "splashImageUrl",
      "deviceToken",
      "updatedAt",
      "createdAt",
      "address",
      "paypalEmail",
      "phoneNumber",
      "gender",
      "birthday",
      "taxId",
      "locale",
      "roles",
      "altAvatarImages",
      "profileBio",
      "allowsChat",
      "currency",
      "appUserType",
      "isVerifiedByYeay",
      "yeayScore",
      "isScoreFrozen"
    ]
  }
}